# === PEPEPOW RPC ===
# IMPORTANT: Set strong credentials here; NEVER commit real secrets.
RPC_URL=http://127.0.0.1:8093
RPC_USER=change_this_user
RPC_PASS=change_this_password

# === API ===
PORT=9193
API_KEY=change_this_api_key

# === Rate limit ===
RATE_MAX=60
RATE_TIME_WINDOW=60000

# === Redis (optional) ===
# Example: redis://127.0.0.1:6379/0
REDIS_URL=

# === ZMQ (optional) ===
# Must match zmqpubrawblock in PEPEPOW.conf
ZMQ_BLOCK=tcp://127.0.0.1:28332
